{
	"name": "vscode-theme-builder",
	"version": "1.0.0",
	"displayName": "Oogway Official",
	"homepage": "https://xavianaxw.com/",
	"description": "VSCode (dark) theme builder inspired by Official Dracula Theme.",
	"publisher": "xavianaxw",
	"license": "MIT",
	"scripts": {
		"build": "node ./scripts/build.js",
		"lint": "node ./scripts/lint.js",
		"package": "vsce package -o ./bin/oogway.vsix",
		"vscode:prepublish": "npm run build",
		"vsce-publish": "vsce publish"
	},
	"maintainers": [
		"Xavian Ang <xavianaxw@gmail.com>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/xavianaxw/vscode-theme-builder.git"
	},
	"bugs": {
		"url": "https://github.com/xavianaxw/vscode-theme-builder/issues"
	},
	"engines": {
		"vscode": "^1.13.0"
	},
	"categories": [
		"Themes"
	],
	"keywords": [
		"dark",
		"theme",
		"color-theme",
		"theme-builder"
	],
	"icon": "icon.png",
	"galleryBanner": {
		"color": "#3c4557",
		"theme": "dark"
	},
	"contributes": {
		"themes": [
			{
				"label": "Oogway",
				"uiTheme": "vs-dark",
				"path": "./theme/oogway.json"
			},
			{
				"label": "Oogway Soft",
				"uiTheme": "vs-dark",
				"path": "./theme/oogway-soft.json"
			}
		]
	},
	"devDependencies": {
		"js-yaml": "^4.1.0",
		"prettier": "^2.6.0",
		"tinycolor2": "^1.4.2",
		"vsce": "^2.7.0"
	}
}
